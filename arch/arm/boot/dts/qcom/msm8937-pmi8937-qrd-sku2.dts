	pinctrl-1 = <&mdss_dsi_suspend>;
	qcom,platform-te-gpio = <&tlmm 24 0>;
	qcom,platform-reset-gpio = <&tlmm 60 0>;
	//vsp-supply = <&tps65132_pos>;
	//vsn-supply = <&tps65132_neg>;
    //panelvddio-supply = <&pm8937_l6>;
    
    
